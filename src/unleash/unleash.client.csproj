<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Olav Nyb√∏</Authors>
    <PackageProjectUrl>https://github.com/onybo/unleash-client-core</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/onybo/unleash-client-core/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/onybo/unleash-client-core.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>asp.net core unleash feature-toggling</PackageTags>
    <Description>.Net Core client for Unleash https://github.com/Unleash/unleash </Description>
    <Version>$(SemVer)</Version>
  </PropertyGroup>

  <ItemGroup>
     <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
     <PackageReference Include="murmurhash" Version="1.0.2" />
  </ItemGroup>

</Project>
